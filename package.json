{
  "name": "gitcaster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "validate-env": "node scripts/validate-env.js",
    "docker:dev": "docker-compose up",
    "docker:build": "docker build -t gitcaster:latest .",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "setup": "./scripts/setup-dev.sh",
    "backup": "./scripts/backup-database.sh",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:prod": "./scripts/deploy.sh production",
    "health-check": "./scripts/check-health.sh",
    "load-test": "node scripts/load-test.js",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@reown/appkit": "^1.8.13",
    "@reown/appkit-adapter-wagmi": "^1.8.13",
    "@tanstack/react-query": "^5.90.7",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.553.0",
    "next": "16.0.1",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.4.1",
    "siwe": "^3.0.0",
    "tailwind-merge": "^3.4.0",
    "viem": "^2.38.6",
    "wagmi": "^2.19.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
